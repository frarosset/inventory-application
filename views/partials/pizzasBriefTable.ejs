<table>
  <thead>
    <tr>
      <th>Pizza</th>
      <th>Ingredients</th>
      <th>Categories</th>
      <th>Availability</th>
      <th>Cost</th>
      <th>Protected</th>
    </tr>
  </thead>
  <tbody>
    <%_ pizzasBriefData.forEach(data => { _%>
    <tr>
      <td><%- include("pizzaLink.ejs", data) %></td>
      <td>
        <%- include("itemsList.ejs", {itemType: 'ingredientLink', items: data.ingredients ?? []}) %>
      </td>
      <td>
        <%- include("itemsList.ejs", {itemType: 'categoryLink', items: data.actual_categories ?? []}) %>
      </td>
      <td><%=data.availability %></td>
      <td><%=data.cost %></td>
      <td><%- include("isProtectedSymbol.ejs", {is_protected: data.is_protected}) %></td>
    </tr>
    <%_ }) _%>
  </tbody>
</table>