<table>
  <thead>
    <tr>
      <th>Category</th>
      <th>Protected</th>
      <th>Actions</th>
    </tr>
  </thead>
  <tbody>
    <%_ categoriesBriefData.forEach(data => { _%>
    <tr>
      <td><%- include("categoryLink.ejs", data) %></td>
      <td><%- include("isProtectedSymbol.ejs", {is_protected: data.is_protected}) %></td>
      <td><%- include("itemActions.ejs", {baseRoute: "/categories", id: data.id}) %></td>
    </tr>
    <%_ }) _%>
  </tbody>
</table>